package while_;

public class DoWhile {

	public static void main(String[] args) {
		//A B C D E F G ~~ X Y Z
		char ch = 'A';
		int count = 0;
		do {
			System.out.print(ch + " ");
			ch++;
			if(count % 7 == 0) {
				System.out.println();
				while(count<30) {
					count++;
				}
			}
		}while(ch<='Z');
	}
}

/*
[문제] A ~ Z 출력하되 1줄에 7개씩 출력하시오
*/